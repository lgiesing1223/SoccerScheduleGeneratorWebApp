<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Soccer Schedule Servlet</display-name>
  <servlet>
    <servlet-name>SoccerScheduleServlet</servlet-name>
    <servlet-class>servlets.SoccerScheduleServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>RedirectServlet</servlet-name>
    <servlet-class>servlets.RedirectServlet</servlet-class>
  </servlet>

<servlet-mapping>
<servlet-name>SoccerScheduleServlet</servlet-name>
<url-pattern>/SoccerSchedule</url-pattern>
</servlet-mapping>
<servlet-mapping>
<servlet-name>RedirectServlet</servlet-name>
<url-pattern>/redirect</url-pattern>
</servlet-mapping>
</web-app>